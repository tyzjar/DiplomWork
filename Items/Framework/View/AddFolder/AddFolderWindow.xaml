<Window x:Class="GUI.Items.Framework.View.AddFolderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.Items.Framework.View"
         Background="{StaticResource MyColor1}"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Subfolders settings" Height="240" Width="340"
        ResizeMode="NoResize"
        KeyDown="KeyEvents">
   <Border Style="{StaticResource BorderStyle}" Padding="3" Margin="10,10">
      <UserControl x:Name="AddFolder" Margin="5,5,5,5"/>
   </Border>
</Window>
